{
  "name": "@inglorious/logo",
  "version": "2.0.1",
  "description": "The Inglorious Coderz logo, remade to be compatible with Inglorious Web.",
  "author": "IceOnFire <antony.mistretta@gmail.com> (https://ingloriouscoderz.it)",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/IngloriousCoderz/inglorious-forge.git",
    "directory": "packages/logo"
  },
  "bugs": {
    "url": "https://github.com/IngloriousCoderz/inglorious-forge/issues"
  },
  "keywords": [
    "inglorious",
    "inglorious-logo",
    "logo",
    "svg",
    "lit-html",
    "inglorious-web",
    "ui",
    "interactive"
  ],
  "type": "module",
  "exports": {
    ".": {
      "types": "./types/index.d.ts",
      "import": "./src/index.js"
    },
    "./style.css": {
      "import": "./src/style.css"
    }
  },
  "files": [
    "src",
    "types"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "format": "prettier --write '**/*.{js,jsx}'",
    "lint": "eslint . --ext js,jsx --report-unused-disable-directives --max-warnings 0",
    "test:watch": "vitest",
    "test": "vitest run"
  },
  "dependencies": {
    "@inglorious/web": "workspace:*"
  },
  "devDependencies": {
    "@inglorious/eslint-config": "workspace:*",
    "prettier": "^3.6.2",
    "vite": "^7.1.3",
    "vitest": "^4.0.15"
  },
  "packageManager": "pnpm@10.15.0",
  "engines": {
    "node": ">= 22"
  },
  "types": "types/index.d.ts"
}
