    "test:watch": "vitest",
    "test": "vitest run"
  },
  "devDependencies": {
    "prettier": "^3.6.2",
    "vite": "^7.1.3",
    "vitest": "^1.6.1"
  },
  "packageManager": "pnpm@10.15.0",
  "engines": {
    "node": ">= 22"
  }
}
